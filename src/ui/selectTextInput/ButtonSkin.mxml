<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
             xmlns:s="library://ns.adobe.com/flex/spark">
         
    <fx:Metadata>
		[HostComponent("ui.selectTextInput.IconButton")] 
	</fx:Metadata>
 
	<s:states> 
		<s:State name="up" /> 
		<s:State name="over" /> 
		<s:State name="down" /> 
		<s:State name="disabled" /> 
	</s:states>  

	<!-- icon -->
	<s:BitmapImage source="@Embed(source='/assets/images/icons/button.png')" includeIn="up, disabled"/>
	<s:BitmapImage source="@Embed(source='/assets/images/icons/button_over.png')" includeIn="over"/>
	<s:BitmapImage source="@Embed(source='/assets/images/icons/button_over.png')" includeIn="down"/>
       
</s:SparkSkin>